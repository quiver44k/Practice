class McLaren720S {
    public String model;
    public int speed;

    public McLaren720S(String model, int speed) {
        this.model = model;
        this.speed = speed;
    }

    public String getData() {
        return model + " " + speed + " mph";
    }
}

class McLarenP1 {
    public String model;
    public int speed;

    public McLarenP1(String model, int speed) {
        this.model = model;
        this.speed = speed;
    }

    public String getData() {
        return model + " " + speed + " mph";
    }
}

public class Main {
    public static void main(String[] args) {
        McLaren720S car1 = new McLaren720S("720S", 212);
        McLarenP1 car2 = new McLarenP1("P1", 217);

        System.out.println(car1.getData());
        System.out.println(car2.getData());
    }
}
